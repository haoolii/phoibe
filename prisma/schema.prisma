generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Record {
  id          String   @id @default(uuid())
  websiteName String
  url         String
  count       Int
  sourceId    String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt()
}